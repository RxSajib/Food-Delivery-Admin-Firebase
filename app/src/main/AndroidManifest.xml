<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mealmonkeyadmin">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MealMonkeyFoodDeleveryJava_Admin">
        <activity
            android:name=".UI.Activity.OrderProduct"
            android:exported="false"
            android:theme="@style/WhiteStatusbar"/>
        <activity
            android:name=".UI.Activity.AllOrder"
            android:exported="false"
            android:theme="@style/WhiteStatusbar" />
        <activity
            android:name=".UI.Activity.AddFood"
            android:exported="false"
            android:theme="@style/WhiteStatusbar" />
        <activity
            android:name=".UI.Activity.AddRestrounts"
            android:exported="true"
            android:theme="@style/WhiteStatusbar" />
        <activity
            android:name=".UI.Activity.ManageFoodMenu"
            android:exported="true"
            android:theme="@style/WhiteStatusbar" />
        <activity
            android:name=".UI.Activity.Home"
            android:exported="true"
            android:theme="@style/WhiteStatusbar" />
        <activity
            android:name=".UI.SignIn"
            android:exported="true"
            android:theme="@style/WhiteStatusbar" />
        <activity
            android:name=".UI.Activity.SplashScreen"
            android:exported="true"
            android:theme="@style/TransprentStatusbar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>